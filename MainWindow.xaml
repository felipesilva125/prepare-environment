<Window x:Class="prepare_environment.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:prepare_environment"
        mc:Ignorable="d"
        Title="MainWindow" Height="481" Width="800" Background="#0D7FA9">
    <Grid>
        <Button Content="Backup" HorizontalAlignment="Left" Margin="22,378,0,0" VerticalAlignment="Top" Height="37" Width="128" Click="ExecuteBackup" Background="#135E8E" Foreground="White" BorderThickness="0" MouseEnter="Button_MouseEnter" MouseLeave="Button_MouseLeave" ClickMode="Release"/>
        <ProgressBar x:Name="BackupProgressBar" Margin="22,19,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Width="251" Height="28" />
        <Button Content="Cancelar" HorizontalAlignment="Left" Margin="190,378,0,0" VerticalAlignment="Top" Height="37" Width="128" Click="CancelBackup" Background="#135E8E" Foreground="White" BorderThickness="0" MouseEnter="Button_MouseEnter" MouseLeave="Button_MouseLeave" ClickMode="Release"/>
    </Grid>
</Window>
